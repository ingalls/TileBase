{
    "name": "tilebase",
    "type": "module",
    "version": "3.0.0",
    "description": "Range based Single File MBTiles like Store",
    "main": "tilebase.js",
    "bin": "cli.js",
    "engines": {
        "node": ">= 16.0.0"
    },
    "scripts": {
        "doc": "documentation build tilebase.js -f html -o docs && documentation lint tilebase.js",
        "lint": "eslint tilebase.js cli.js lib/**/*.js test/**/*.js",
        "lint-doc": "documentation lint tilebase.js",
        "test": "tape test/**.test.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/openaddresses/TileBase.git"
    },
    "keywords": [
        "mbtiles",
        "openaddresses",
        "mapbox",
        "mvt",
        "vector",
        "tiles"
    ],
    "author": "Nick Ingalls",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/openaddresses/TileBase/issues"
    },
    "homepage": "https://github.com/openaddresses/TileBase#readme",
    "devDependencies": {
        "documentation": "^13.2.5",
        "eslint": "^8.1.0",
        "eslint-plugin-node": "^11.1.0",
        "tape": "^5.2.2"
    },
    "dependencies": {
        "@mapbox/mbtiles": "^0.12.1",
        "@mapbox/tile-cover": "^3.0.2",
        "@mapbox/tilebelt": "^1.0.2",
        "@mapbox/vector-tile": "^1.3.1",
        "@turf/bbox-polygon": "^6.5.0",
        "@turf/centroid": "^6.5.0",
        "@turf/helpers": "^6.3.0",
        "ajv": "^8.2.0",
        "aws-sdk": "^2.900.0",
        "minimist": "^1.2.5",
        "node-fetch": "^3.2.0",
        "nyc": "^15.1.0",
        "pbf": "^3.2.1"
    }
}
